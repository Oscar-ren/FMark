{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node www/development.js",
    "compile": "babel --presets es2015-loose,stage-1 --plugins transform-runtime src/ --out-dir app/ --source-maps",
    "watch-compile": "node -e \"console.log('<npm run watch-compile> no longer need, use <npm start> command direct.');console.log();\"",
    "watch": "webpack --process --watch"
  },
  "dependencies": {
    "thinkjs": "2.2.x",
    "babel-runtime": "6.x.x",
    "source-map-support": "0.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.21.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsonp": "^0.2.0",
    "memory-fs": "^0.3.0",
    "mime": "^1.3.4",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2"
  },
  "author": "",
  "license": "ISC"
}
